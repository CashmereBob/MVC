@model IEnumerable<MVCLabb.Models.PhotoModel>

@{
    ViewBag.Title = "Index";
}

<h2>The Gallery</h2>

<p>
    @Html.ActionLink("Add a Photo", "Create")
</p>
<div id="masonarycontainer" class="row" style="display: flex; flex-flow: column wrap; justify-content: flex-start; align-items: flex-start; margin: 0 auto; height: 500px; overflow: hidden;">

    @foreach (var item in Model)
            {
        @Html.Partial("_Thumbnails", item)
    }

</div>
@section Scripts {

    <script src="~/Scripts/MasonaryJavaScript.js"></script>

}
